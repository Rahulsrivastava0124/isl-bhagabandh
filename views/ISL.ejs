<%- include("./Nav")-%>
  <style>
    .contact_container_height {
      height: 500px;
    }
  </style>
  <h1 class="text-center">ISL</h1>
  <div class="container d-flex flex-wrap justify-content-evenly">

    <a href="/new_addmission"> <button type="button" class="btn btn-primary btn-lg p-5 m-2 box_shodaw"><i
          class="bi bi-person-plus-fill h1"></i><br>
        <h6>New Addimission</h6>
      </button></a>
    <a href="/re-addmission"> <button type="button" class="btn btn-success btn-lg p-5 m-2 box_shodaw"><i
          class="bi bi-person-plus-fill h1"></i><br>
        <h6>Re-Addimission</h6>
      </button></a>
    <a href="/fee-submission"> <button type="button" class="btn btn-danger btn-lg p-5 m-2 box_shodaw"><i
          class="bi bi-person-plus-fill h1"></i><br>
        <h6>Fee-Submission</h6>
      </button></a>
    <a href=" 9th_addmission"> <button type="button" class="btn btn-warning btn-lg p-5 m-2 box_shodaw"><i
          class="bi bi-person-plus-fill h1"></i><br>
        <h6>9th Addimission</h6>
      </button></a>
    <a href="10th_addmission"> <button type="button" class="btn btn-secondary btn-lg p-5 m-2 box_shodaw"><i
          class="bi bi-person-plus-fill h1"></i><br>
        <h6>10th Addimission</h6>
      </button></a>
  </div>
  <div class="container w-50">
    <h2 class="bg-secondary rounded-4 sticky-top text-center w-100 ">Query</h2>

    <div class="m-auto p-3 list-group overflow-auto contact_container_height ">
      <div class="d-flex">
        <button class="btn btn-primary m-2 " type="button" data-bs-toggle="collapse" data-bs-target="#collapseExample"
          aria-expanded="false" aria-controls="collapseExample1">
          More..
        </button>
        <button class="btn btn-primary m-2" type="button" data-bs-toggle="collapse" data-bs-target="#collapseExample"
          aria-expanded="false" aria-controls="collapseExample1">
          More..
        </button>
      </div>
      <% data.forEach(element=> { %>
        <a class="list-group-item list-group-item-action rounded-3 shadow-lg p-3 mb-3 bg-body " aria-current="true">
          <div class="d-flex w-100 justify-content-between">
            <h5 class="mb-1">
              <%=element.Contact[0].Username %>
            </h5>
            <small>
              <%=element.Contact[0].Date%>
            </small>
          </div>
          <p class="mb-1">
            <%= element.Contact[0].Description %>
          </p>
          <small>
            <%= element.Contact[0].PhoneNumber %>
          </small>
          <small class="">
            <div class="collapse mb-2" id='collapseExample<%=element.Contact[0]._id%>'>
              <div class="card card-body">
                <%= element.Contact[0].Email %><br>
                  <%= element.Contact[0].City %><br>
                    <%= element.Contact[0].Address %><br>
              </div>
            </div>
          </small>
          <div class="d-grid gap-2 d-md-flex justify-content-md-end">
            <button class="btn btn-primary m-2 " type="button" data-bs-toggle="collapse"
              data-bs-target='#collapseExample<%= element.Contact[0]._id %>' aria-expanded="false" aria-controls="collapseExample1">
              More..
            </button>
            <button class="btn btn-success" type="button"><i class="bi bi-check-circle"></i></button>
            <button class="btn btn-danger" type="button"><i class="bi bi-trash"></i></button>

          </div>
        </a>

        <% }) %>

    </div>
  </div>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
    crossorigin="anonymous"></script>